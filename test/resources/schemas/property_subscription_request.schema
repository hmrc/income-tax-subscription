{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Income Tax Self Assessment Properties Request\n\nSchema",
  "description": "Income Tax Self Assessment Properties Request\n\nSchema",
  "type": "object",
  "properties": {
    "utr": {
      "description": "Unique taxpayer reference number",
      "type": "string",
      "pattern": "^[0-9]{10}$"
    },
    "numOfPropRented": {
      "description": "Total number of properties rented.",
      "type": "string",
      "pattern": "^[0-9]{1,3}$"
    },
    "numOfPropRentedInUK": {
      "description": "No.Of Properties Rented In UK",
      "type": "string",
      "pattern": "^[0-9]{1,3}$"
    },
    "numOfPropRentedInEEA": {
      "description": "No.Of Properties Rented In EEA",
      "type": "string",
      "pattern": "^[0-9]{1,3}$"
    },
    "numOfPropRentedInNonEEA": {
      "description": "No. Of Properties Rented In Non-EEA",
      "type": "string",
      "pattern": "^[0-9]{1,3}$"
    },
    "emailAddress": {
      "type": "string",
      "format": "email",
      "minLength": 3,
      "maxLength": 132
    }
  }
  "additionalProperties": false
}